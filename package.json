{
  "name": "split-man21",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:safe": "npm run test && npm run e2e && npm run build",
    "build:prod": "npx rimraf dist && npm run cp-libs && ng build --configuration production",
    "build:safe:prod": "npm run test:headless && npm run e2e && npm run build:prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadless --watch=false",
    "e2e": "npx ts-node e2e/main_e2e.ts",
    "clear-chromium": "clearChromium.sh",
    "cp-libs": "npx cp-cli node_modules/bootstrap/dist src/assets/bootstrap && npx make-dir-cli src/assets/font-awesome && npx cp-cli node_modules/font-awesome/css/ src/assets/font-awesome/css && npx cp-cli node_modules/font-awesome/fonts src/assets/font-awesome/fonts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "fontawesome": "^5.6.3",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.12",
    "@angular/cli": "~12.2.12",
    "@angular/compiler-cli": "~12.2.0",
    "@types/bootstrap": "^5.1.6",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "chai": "^4.3.4",
    "cp-cli": "^2.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "puppeteer": "^11.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.3.5"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.14"
  }
}
